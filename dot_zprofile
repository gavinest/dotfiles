# git aliases
alias gst="git status"
alias ga="git add"
alias gc="git commit"
alias gpo='git push origin $(git branch --show-current)'
alias glp="git log --pretty=oneline"
alias gd="git diff"

# gcloud aliases
alias gce="gcloud composer environments"
alias gbce="gcloud beta composer environments"
alias gs="gsutil"

# other
alias cz="chezmoi"
alias tf="terraform"
alias dc="docker-compose"

